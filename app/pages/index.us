<!DOCTYPE html>
<html ng-app="app">
<head>
  <base href="/"></base>
  <title><%= pkg.name %></title>
  <link rel="stylesheet" type="text/css" href="/css/app.css" media="all"/>
</head>
<body>
<div class="off-canvas-wrap move-right" data-offcanvas="">
  <div class="inner-wrap">
    <!-- Off Canvas Menu -->
    <aside class="left-off-canvas-menu">

      <!-- TODO: only insert this if the user is logged in -->
      <div class="icon-bar vertical five-up">
        <a class="item">
          <i class="fi-home"></i>
          <label>Dashboard</label>
        </a>
        <a class="item">
          <i class="fi-home"></i>
          <label>Create a Ride</label>
        </a>
        <a class="item">
          <i class="fi-home"></i>
          <label>My Rides</label>
        </a>
        <a class="item">
          <i class="fi-home"></i>
          <label>Browse Rides</label>
        </a>
        <a class="item">
          <i class="fi-home"></i>
          <label>Music</label>
        </a>
      </div>

    </aside>
    <div class="row header content">
      <div id="view" ng-view></div>
    </div>
  </div>
</div>

<script type="text/javascript" src="/js/app.js"></script>
</body>
</html>
